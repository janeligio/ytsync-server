{
  "name": "ytsync_server",
  "engines": {
    "node": "14.17.5"
  },
  "version": "1.0.0",
  "description": "HTTP server for creating synchronized youtube sessions.",
  "main": "src/index.ts",
  "scripts": {
    "postinstall": "yarn tsc",
    "deploy": "git push heroku master",
    "start": "node dist/index.js",
    "tsc": "./node_modules/typescript/bin/tsc",
    "dev": "nodemon",
    "dev-warnings": "tsc && node --trace-warnings dist/index.js"
  },
  "keywords": [
    "YTsync",
    "YouTube Sync",
    "youtube",
    "Youtube",
    "Sync",
    "Synchronized"
  ],
  "author": "Jan Iverson Eligio",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "socket.io": "^4.4.1",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0"
  }
}
